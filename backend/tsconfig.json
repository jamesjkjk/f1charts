{
    "compilerOptions": {
      "target": "ES2020",
      "module": "NodeNext",
      "moduleResolution":"NodeNext",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "lib":["dom","es2020","esnext"],
      "skipLibCheck":true,
      "resolveJsonModule":true,
      "noUncheckedIndexedAccess": true,
    },
    "exclude":["node_modules","dist"],
    "include":["src/**/*.ts"]
  }
  